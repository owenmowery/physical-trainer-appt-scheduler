<h1>Sign Up as a Personal Trainer</h1>
<%= form_with model: @trainer, local: true do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %><br>

    <%= f.label :email %>
    <%= f.email_field :trainer_email %><br>

    <%= f.label :gym_name %>
    <%= f.text_field :gym_name %><br>

    <%= f.label :password %>
    <%= f.password_field :password %><br>

    <%= f.submit %>
<% end %>